# vi:syntax=sh

HAB_TARGET=${HAB_TOOLCHAIN:-x86_64-linux}

load() {
  hab svc load $HAB_ORIGIN/teal-iron-kitten
}

unload() {
  hab svc unload $HAB_ORIGIN/teal-iron-kitten
}

build() {
  hab pkg exec $HAB_ORIGIN/hab-plan-build "$@"
}


